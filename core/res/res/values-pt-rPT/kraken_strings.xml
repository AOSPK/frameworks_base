    <!-- Global actions -->
    <string name="reboot_to_bootloader_title">Reiniciar bootloader</string>
    <string name="reboot_to_bootloader_message">Reiniciar no bootloader\u2026</string>
    <string name="reboot_to_recovery_title">Reiniciar em Recuperação</string>
    <string name="reboot_to_recovery_message">A reiniciar para a recuperação\u2026</string>
    <string name="reboot_system_title">Reiniciar o sistema</string>
